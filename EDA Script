#Import data set
#IMPORTANT CHANGE VALUE OF FILENAME ACCORDING TO IMPORTED DATA SET
file_name <- ASSESSMENT_DATA

#Create data frame
student_num <- file_name$`Student Number`
gender <- file_name$Gender
age <- file_name$Age
f_subject <- file_name$`Favorite Subject`
f_color <- file_name$`Favorite Color`
height <- file_name$`Height (centimeters)`
weight <- file_name$`Weight (kilograms)`
grade_math <- file_name$`General Mathematics`
grade_stat <- file_name$`Statistics and Probability`
grade_sci <- file_name$`Earth Science`
grade_mil <- file_name$`Media and Information Literacy`

my_data_frame <- data.frame(student_num, gender, age, f_subject, f_color, height, weight, grade_math, grade_stat, grade_sci, grade_mil)

#Data Frames
#Males who like red and are at least 21 years old
subset_1 <- subset(my_data_frame, gender == "Male" & f_color == "Red" & age >= 21)
#Males who like color blue and at most 60kilos
subset_2 <- subset(my_data_frame, gender == "Male" & f_color == "Blue" & weight <= 60)
#Females who like color yellow and is taller than 150cm
subset_3 <- subset(my_data_frame, gender == "Female" & f_color == "Yellow" & height > 150)
#Females who like math and the color green
subset_4 <- subset(my_data_frame, gender == "Female" & f_subject == "Math" & f_color == "Green")
#Students who like math and grades in GenMath and Statistics are greater than 95
subset_5 <- subset(my_data_frame, f_subject == "Math" & (grade_math & grade_stat) > 95)
#Students who like science and grade in EarthSci is greater than 95
subset_6 <- subset(my_data_frame, f_subject == "Science" & grade_sci > 95)
#Students who like english and grade in MIL is greater than 95
subset_7 <- subset(my_data_frame, f_subject == "English" & grade_mil > 95)

#Matrices
#Create vectors for data, rows and columns
data <- c(1:20)
rlen <- c("Red", "Blue", "Yellow", "Green")
clen <- c("A", "B", "C", "D", "E")

#Create matrix
data_matrix <- matrix(data, nrow = 4, ncol = 5, byrow = TRUE, dimnames = list(rlen, clen))

#Print matrix
data_matrix

#Extracting row and column of matrix
data_matrix[, c(1, 3, 4)]
data_matrix[c(2, 3), ]
